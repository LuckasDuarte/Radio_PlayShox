{
  "expo": {
    "name": "Radio-Shox",
    "slug": "Radio-Shox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./telas/images/icone_app.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./telas/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
